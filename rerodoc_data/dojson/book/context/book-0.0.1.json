{
    "@context": {
        "dcmitype": "http://purl.org/dc/dcmitype/",
        "bibo": "http://purl.org/ontology/bibo/",
        "dc": "http://purl.org/dc/elements/1.1/",
        "dct": "http://purl.org/dc/terms/",
        "edm": "http://www.europeana.eu/schemas/edm/",
        "foaf": "http://xmlns.com/foaf/0.1/",
        "rdau": "http://rdaregistry.info/Elements/u/",
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns# ",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",

        "type": "@type",
        "recid": "@id",
        "full": "@value",
        "lang": "@language",
        
        "media_type": {
            "@id": "rdau:mediaType",
            "@type": "@id"
            },

        "@base": "http://doc.rero.ch/record/",

        "text": "dcmitype:Text",
        "book": "bibo:Book",
        "bibrec": "dct:BibliographicResource",

        "language": {
            "@id": "dct:language"
        },
        "title": "dct:title",
        "other_title": "dct:alternative",
        "authors": {
            "@id": "dc:creator"
        },

        "isbn10": "bibo:isbn10",
        "isbn13": "bibo:isbn13",
        
        "rero_id": {
            "@id": "dct:hasFormat",
            "@type": "@id"
        },
        
        "edition": "bibo:edition",
        "publication": "rdau:publicationStatement",
        "publication_date": "dct:issued",
        "collation": "dc:format",
        "series": "dct:bibliographicCitation",
        "meeting": "dc:contributor",
        "corporate": "dc:contributor",
        "udc": {
            "@id": "dct:subject",
            "@type": "@id"
        },
        "uri": "@set"

    }
}
